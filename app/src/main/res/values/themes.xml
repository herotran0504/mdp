<resources>

    <style name="Theme.MDP" parent="Theme.Material3.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#EFD48F</item>
        <item name="colorPrimaryVariant">#EFD48F</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
    </style>

    <style name="EditTextTheme">
        <item name="colorControlNormal">#948C9E</item>
        <item name="colorControlActivated">#948C9E</item>
        <item name="colorControlHighlight">#948C9E</item>
    </style>

</resources>